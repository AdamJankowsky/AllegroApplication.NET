@model AllegroWebAplication.AllegroModels.AllegroSoldItems

@{
    ViewBag.Title = "Sprzedane przedmioty";
}

<h2>Sprzedane przedmioty</h2>


<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>#</th>
            <th>Tytuł</th>
            <th>Cena</th>
            <th>Ilość</th>
            <th>Kupił</th>
        </tr>
    </thead>
    @foreach (var item in Model.SoldItems)
    {
        <thead>
            <tr>
                <th>@item.ItemId</th>
                <th>@item.ItemTitle</th>
                <th>@item.ItemPrice</th>
                <th>@item.ItemQuantity</th>
                <th>@item.ItemBuyer.UserLogin</th>
            </tr>
        </thead>
    }
</table>