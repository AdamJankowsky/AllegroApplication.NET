@model AllegroWebAplication.AllegroModels.AllegroWonItems

@{
    ViewBag.Title = "Kupione przedmioty";
}

<h2>Kupione przedmioty</h2>

<hr />
<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>#</th>
            <th>Tytuł</th>
            <th>Ilość</th>
            <th>Cena</th>
            <th>Sprzedawca</th>
        </tr>
    </thead>
    @foreach (var item in Model.WonItems){
        <tr>
            <td>@item.ItemId</td>
            <td>@item.ItemTitle</td>
            <td>@item.ItemQuantity</td>
            <td>@item.ItemPrice</td>
            <td>@item.ItemSeller.SellerLogin</td>
        </tr>
    }

</table>
