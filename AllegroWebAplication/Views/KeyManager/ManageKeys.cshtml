@model IEnumerable<AllegroWebAplication.ViewModels.KeyManagerKeyViewModel>

@{
    ViewBag.Title = "Show Keys";
}

<h2>Show Keys</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.ActionLink("Delete", "DeleteKey", new { id=item.Id })
        </td>
    </tr>
}

</table>
